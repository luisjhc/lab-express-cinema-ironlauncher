<div class="text">
  <h1>Ironhack Cinema</h1>
  <p>Click on "See more" to check the showtimes</p>
  <h4>Welcome {{user.username}}</h4>
  <a href="/profile/edit">Edit your profile</a>
  <a href="/auth/logout">Logout</a>
  <a href="/">Go back</a>
  <a href="/create-movie">Create a movie</a>
</div>

<div class="allMovies">
  {{#each allMovies}}
  <div class="containerMovies">
    <img src="{{this.image}}" alt="movieImage">
    <h3>{{this.title}}</h3>
    <a href="/movies/{{this.id}}">See more</a>
  </div>
  {{/each}}
</div>